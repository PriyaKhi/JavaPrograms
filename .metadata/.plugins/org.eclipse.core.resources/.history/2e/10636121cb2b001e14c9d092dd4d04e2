package PriyaPractice;

import java.util.Scanner;

public class GeekForGeekNonRepeatingCharacter {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the String");
		String c = sc.next();

		c = c.replaceAll("//s", "");
		char[] arr = c.toCharArray();
		int length = 0;
		for (int i = 0; i < arr.length; i++) {
			int count = 0;
			for (int k = 1; k < arr.length; k++) {
				if (arr[i] == arr[k]) {
					count = 1;
					break;
				}
				length++;

			}
			System.out.print("Me" + length);
			System.out.print("M12" + arr.length);
			if (length == arr.length) {
				System.out.println(arr[i]);
				System.exit(0);
			}
			if (count == 0) {
				System.out.println(arr[i]);
				System.exit(0);
			}
		}
		System.out.println("$");
	}
}