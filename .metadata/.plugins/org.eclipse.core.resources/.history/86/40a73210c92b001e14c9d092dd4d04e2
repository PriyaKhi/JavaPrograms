package PriyaPractice;

import java.util.Scanner;

public class GeekForGeekNonRepeatingCharacter {

	Scanner sc = new Scanner(System.in);System.out.print("Enter the String");
	String c = sc.next();

	c=c.replaceAll("//s","");
	char[] arr = c.toCharArray();
	int count = 0;

	for(
	int i = 0;i<arr.length;i++)
	{

		for (int k = 1; k < arr.length; k++) {
			if (arr[i] == arr[k]) {
				count = 1;
				break;
			}
		}
		if (count == 0) {
			System.out.print(arr[i]);
		}
	}System.out.print("$");
}}