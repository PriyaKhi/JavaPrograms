package Interview;

public class secondLargestInArray {

	public static void main(String[] args) {
		int arr[] = { 4, 781, 8, 99, 103 };

		int max = 0;
		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] < arr[j]) {
					max = arr[j];
                    
				}

			}

		}
	}
}
