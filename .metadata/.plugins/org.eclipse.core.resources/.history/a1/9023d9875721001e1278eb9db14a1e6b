package Interview;

import java.util.Scanner;

public class midNumber {

	public static int ReturnMiddleNumber(int a, int b, int c) {

		if ((a >= b && b >= c) || (c >= b && b >= a))
			return b;
		else if ((b >= a && a >= c) || (c >= a && a >= b))
			return a;
		else if ((b >= c && c >= a) || (a >= c && c >= b))
			return c;
		return -1;
	}

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		System.out.print("Enter any three number");

		int a = in.nextInt();
		int b = in.nextInt();
		int c = in.nextInt();
		System.out.print(ReturnMiddleNumber(a, b, c));

	}

}
