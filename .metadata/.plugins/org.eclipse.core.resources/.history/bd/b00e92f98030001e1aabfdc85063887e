package PriyaPractice;

public class secondLargestArray {

	public static void main(String args[]) {
		int[] arr = { 100, 90, 20, 30, 40, 40, 50 };
		int firstMax = arr[0];
		int secondMax = arr[0];
		for (int i = 1; i < arr.length; i++) {
			if (arr[i] > firstMax) {
				secondMax = firstMax;
				firstMax = arr[i];
			} else {
				secondMax=arr[i];				
			}

		}
		System.out.println("a " + firstMax);
		System.out.println("b " + secondMax);
	}

}
