package PriyaPractice;

import java.util.HashSet;

public class duplicatePrograms {

	public static void main(String[] args) {
		removeDuplicatesFromArrayUsingHashSet();

		removeDuplicatesFromArray();
	}

	public static void removeDuplicatesFromArrayUsingHashSet() {
		int arr[] = { 10, 14, 14, 10, 55, 66, 55, 11, 66 };
		HashSet<Integer> hs = new HashSet<>();
		for (int i = 0; i < arr.length; i++) {
			hs.add(arr[i]);
		}
		System.out.print(hs);
	}

	public static void removeDuplicatesFromArray() {
		int arr[] = { 10, 14, 14, 10, 55, 66, 55, 11, 66 };

		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] == arr[j]) {

				}
			}
		}
	}
}
