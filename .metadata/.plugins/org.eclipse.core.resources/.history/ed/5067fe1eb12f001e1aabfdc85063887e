package PriyaPractice;

public class removeDuplicateArray {

	public static void main(String[] args) {

		int[] arr1 = { 10, 10, 20, 30, 40, 40, 50 };
HashSet <int> h=new HashSet<>();
		for (int i = 0; i < arr1.length; i++) {
			for (int j = i + 1; j < arr1.length; j++) {
				if (arr1[i] == arr1[j]) {
					arr1[j] = '0';
				}
			}

		}
	}
}
