package PriyaPractice;

public class sumOfNumbersinString {
	public static void main(String[] args) {
		String s = "1414Priya151!#%Khiwal";

		char[] arr = s.toCharArray();
		int sum = 0;
		for (int i = 0; i < arr.length; i++) {
			if (Character.isDigit(arr[i])) {
				sum += arr[i];
			}

		}

	}

}
